{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-console": "off"
    }
}
